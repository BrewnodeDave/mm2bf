{
  "name": "malt-miller-inventory",
  "version": "1.0.0",
  "description": "Extract ingredients from Malt Miller invoices and sync with Brewfather",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "sync": "node src/index.js",
    "extract": "node src/extract-costs.js",
    "match": "node src/match-ingredients.js",
    "test": "node src/test.js",
    "parse-only": "node src/test.js --parse-only",
    "connection-test": "node src/test.js --connection-only",
    "dry-run": "node src/test.js --dry-run",
    "webapp": "cd webapp && npm start",
    "webapp-dev": "cd webapp && npm run dev"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "pdf-poppler": "^0.2.1",
    "pdf2json": "^3.2.0",
    "pdf2pic": "^3.2.0"
  },
  "keywords": [
    "brewfather",
    "pdf",
    "ingredients",
    "brewing"
  ],
  "author": "",
  "license": "MIT"
}
